@model Profile.UI.Models.Profile.ArtefactsViewModel

<button class="prf-edit btn-link" type="button" id="" title="Редактировать">
    <span class="glyphicon glyphicon-pencil"></span>
</button>
<div class="row">
    <div class="prf-add-inform">
        <h5>Список артефактов</h5>
    </div>
</div>
<div class="row">
    <div class="prf-add-inform prf-center-inline prf-block-anchor" id="FilesLinks">
        @Html.Partial("~/Views/Artefact/_PartialEditFiles.cshtml", Model.Files)
        @Html.Partial("~/Views/Artefact/_PartialEditLinks.cshtml", Model.Links)
    </div>
</div>
<!-- Upload type Menu -->
<div class="row prf-block-wrap">
    <div class="prf-add-artifacts" id="addArtifacts">
        <div class="prf-legend"><span>Добавить артефакты</span></div>
        <div class="prf-add-inform prf-center-inline">
            <a class="prf-artifact" href="#loadFileModal" id="loadFile" data-toggle="modal" data-target="#loadFileModal" data-backdrop="static">
                <span class="prf-artifact-back">
                    <span class="glyphicon glyphicon-file prf-artifact-icon"></span>
                </span>
                <span class="prf-signature">Файл</span>
            </a>
            <a class="prf-artifact" href="#artifacts">
                <span class="prf-artifact-back">
                    <span class="glyphicon glyphicon-screenshot prf-artifact-icon"></span>
                </span>
                <span class="prf-signature">Скриншот</span>
            </a>
            <a class="prf-artifact" href="#artifacts">
                <span class="prf-artifact-back">
                    <img src="/images/artifact-table.png" alt="">
                </span>
                <span class="prf-signature">Таблица</span>
            </a>
            <a class="prf-artifact" href="#artifacts">
                <span class="prf-artifact-back">
                    <img src="/images/artifact-presentation.png" alt="">
                </span>
                <span class="prf-signature">Презентация</span>
            </a>
            <a class="prf-artifact" href="#loadLinkModal" id="loadLink" data-toggle="modal" data-target="#loadLinkModal" data-backdrop="static">
                <span class="prf-artifact-back">
                    <span class="glyphicon glyphicon-link prf-artifact-icon"></span>
                </span>
                <span class="prf-signature">Ссылка</span>
            </a>
        </div>
    </div>
</div>
<!-- end of Upload type Menu -->
<!-- Block of Buttons -->
<div class="row prf-underline prf-button-edit">
    <button type="button" class="btn btn-warning prf-cansel">Отмена</button>
</div>
<!-- end of Block of Buttons -->
<!-- File Template -->
<a id="blankFile" class="prf-artifact prf-file hidden" href="#artifacts" data-file-id="0" data-file-name="" data-file-size="0">
    <span class="prf-artifact-back">
        <span class="prf-artifact-icon">
            <img src="/images/file-other.png" alt="">
            <button class="prf-trash btn-link" type="button" id="" title="Удалить" disabled="disabled">
                <span class="glyphicon glyphicon-trash"></span>
            </button>
        </span>
    </span>
    <span class="prf-signature">Мой Скриншот</span>
</a>
<!-- end of File Template -->
<!-- Link Template -->
<a id="blankLink" class="prf-artifact prf-link hidden" href="#artifacts" data-link-id="0" data-link="">
    <span class="prf-artifact-back">
        <span class="prf-artifact-icon">
            <span class="glyphicon glyphicon-link prf-artifact-icon"></span>
            <button class="prf-trash btn-link" type="button" id="" title="Удалить" disabled="disabled">
                <span class="glyphicon glyphicon-trash"></span>
            </button>
        </span>
    </span>
    <span class="prf-signature">Моя Ссылка</span>
</a>
<!-- end of Link Template -->