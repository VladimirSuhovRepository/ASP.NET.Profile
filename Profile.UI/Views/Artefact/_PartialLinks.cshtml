@model IEnumerable<Profile.UI.Models.Profile.LinkViewModel>

<!-- Links list -->
@foreach (var link in Model)
{
    <a class="prf-artifact" href="@link.Url" data-link-id="@link.Id" target="_blank">
        <span class="prf-artifact-back">
            <span class="prf-artifact-icon">
                <span class="glyphicon glyphicon-link prf-artifact-icon"></span>
            </span>
        </span>
        <span class="prf-signature">@link.Description</span>
    </a>
}
<!-- end of Links list -->
